<Window x:Class="LoadCell.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
        xmlns:local="clr-namespace:LoadCell"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">

    <Grid>
        <StackPanel x:Name="Main_SP">
            <TextBlock x:Name="Value_TextBlock" Width="auto" Height="25"/>
            <Button x:Name="Test_Button" Width="auto" Height="25" Content="TEST" Click="Test_Button_Click"/>
            <lvc:CartesianChart x:Name="TestChart" Series="{Binding Series}" Height="400" Width="800">

            </lvc:CartesianChart>
            <ListBox>
                
            </ListBox>
        </StackPanel>
    </Grid>
</Window>
